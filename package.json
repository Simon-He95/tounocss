{
  "publisher": "simonhe",
  "name": "to-unocss",
  "displayName": "To Unocss",
  "version": "0.0.19",
  "description": "Vscode extension transform css to unocss",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Simon-He95",
  "homepage": "https://github.com/Simon-He95/tounocss#readme",
  "repository": "https://github.com/Simon-He95/tounocss",
  "bugs": {
    "url": "https://github.com/Simon-He95/tounocss/issues"
  },
  "keywords": [
    "unocss",
    "css",
    "css to unocss",
    "transform-to-unocss"
  ],
  "categories": [
    "Formatters",
    "Snippets",
    "Other"
  ],
  "main": "./out/extension.js",
  "icon": "icon.png",
  "engines": {
    "node": "14.18.1",
    "vscode": "^1.73.0"
  },
  "activationEvents": [
    "onLanguage:html",
    "onLanguage:svelte",
    "onLanguage:jsx",
    "onLanguage:tsx",
    "onLanguage:astro",
    "onLanguage:vue",
    "onLanguage:swan",
    "onLanguage:wxml",
    "onLanguage:axml"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.ToUnocss",
        "title": "toUnocss"
      },
      {
        "command": "extension.InlineStyleToUnocss",
        "title": "styleToUnocss"
      }
    ],
    "keybindings": [
      {
        "command": "extension.ToUnocss",
        "key": "cmd+Alt+p"
      },
      {
        "command": "extension.InlineStyleToUnocss",
        "key": "Alt+shift+p"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "npm compile && node ./node_modules/vscode/bin/test",
    "build": "vsce package",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.md,.json --max-warnings 0 --cache",
    "lint:fix": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.md,.json --max-warnings 0 --cache --fix",
    "release": "npm build && npx bumpp --commit --tag --push --all && vsce publish --no-dependencies"
  },
  "dependencies": {
    "vscode-nls": "^4.1.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.37.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.1.28",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vscode/vsce": "^2.18.0",
    "bumpp": "^9.0.0",
    "del": "^4.1.1",
    "esbuild": "^0.17.12",
    "eslint": "^8.24.0",
    "event-stream": "^4.0.1",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "rollup": "^3.20.0",
    "run-sequence": "^2.2.1",
    "transform-to-unocss": "^0.0.55",
    "tslint": "^5.16.0",
    "typescript": "^4.8.4",
    "vscode-nls-dev": "^3.2.6"
  }
}
